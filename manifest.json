{
  "manifest_version": 3,
  "name": "Sci-Fi Clock New Tab",
  "version": "1.0",
  "description": "Replaces new tab page with a sci-fi clock",
  "chrome_url_overrides": {
    "newtab": "clock.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}